

#include <iostream>
using namespace std;

int main()
{

   //simple calculator
	int a;
	int b;


	cout << "enter 2 numbers:\n";
	cin >> a >> b;
	cout << a << "+" << b << "=" << a + b << endl;
	cout << a << "-" << b << "=" << a - b << endl;
	cout << a << "*" << b << "=" << a * b << endl;
	cout << a << "/" << b << "=" << a / b << endl;
	cout << a << "%" << b << "=" << a % b << endl;
}
